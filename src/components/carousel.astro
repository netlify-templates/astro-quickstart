---
export interface Props {
	carouselIndex: string;
}

var { carouselIndex, dataColumns } = Astro.props as Props;

carouselIndex = parseInt(carouselIndex)
dataColumns = parseInt(dataColumns)

var forwFunction = `javascript:goBack(${carouselIndex});`
var backFunction = `javascript:goForw(${carouselIndex});`

console.log(dataColumns)
---
<div class="carousel" data-index={carouselIndex} data-columns={dataColumns}>
  <slot />
</div>